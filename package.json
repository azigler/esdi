{
  "name": "esdi",
  "version": "1.0.0",
  "description": "ES6 Discord bot framework",
  "author": "Andrew Zigler <andrewzigler@gmail.com> (https://ko-fi.com/andrewzigler)",
  "main": "index.js",
  "homepage": "https://azigler.github.io/esdi/",
  "repository": {
    "type": "git",
    "url": "git://github.com/azigler/esdi.git"
  },
  "engines": {
    "node": ">=12.18.2"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nodemon ./scripts/dev.js",
    "run": "node ./scripts/dev.js",
    "lint": "eslint . --ext .js --fix --ignore-pattern '**/node_modules/**'",
    "merge-main-to-dev": "git checkout develop && git merge --no-ff main",
    "merge-dev-to-main": "git checkout main && git merge --no-ff develop",
    "docs": "jsdoc -c ./config/.jsdoc.json -R ./config/.home.md -u config",
    "clean": "npm run clean:docs && npm run clean:data",
    "clean:docs": "rm -rf out && mkdir out && cd out && touch .gitkeep",
    "clean:data": "rm -rf data && mkdir data && cd data && touch .gitkeep"
  },
  "dependencies": {
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "pouchdb": "^7.2.2"
  },
  "devDependencies": {
    "docdash": "^1.2.0",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-standard": "^4.1.0",
    "jsdoc": "^3.6.6",
    "standard": "^16.0.3"
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "extends": [
      "standard"
    ],
    "plugins": [
      "standard"
    ],
    "rules": {
      "no-console": 0,
      "array-callback-return": 0
    }
  }
}
